{
  "expo": {
    "name": "SareeMatcher",
    "slug": "SareeMatcher",
    "owner": "saiganeshgunda",
    "platforms": ["android"],
    "extra": {
      "API_BASE": "https://sareematcherbackend.onrender.com",
      "router": {},
      "eas": {
        "projectId": "73c8d3e0-11a9-4a4c-a3ae-3c2cd64cc0ee"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sareematcher",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
